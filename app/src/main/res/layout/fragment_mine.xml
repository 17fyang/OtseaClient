<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >

    <RelativeLayout
            android:layout_width="410dp"
            android:layout_height="98dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:id="@+id/relativeLayout8"
            app:layout_constraintHorizontal_bias="0.0">

        <ImageView
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:id="@+id/mine_head_image"
                android:layout_marginTop="18dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginStart="38dp"
                app:srcCompat="@drawable/unload"/>

        <TextView
                android:text="立即登录"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textView11"
                android:layout_toEndOf="@+id/mine_head_image"
                android:layout_marginStart="16dp"
                android:layout_alignTop="@+id/mine_head_image"
                android:layout_marginTop="8dp"
                android:textSize="20sp"/>

        <TextView
                android:text="登录更精彩"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/mine_signature"
                android:layout_marginTop="5dp"
                android:layout_below="@+id/textView11"
                android:layout_marginStart="0dp"
                android:layout_alignStart="@+id/textView11"
                android:textSize="12sp"/>

        <ImageView
                android:layout_width="39dp"
                android:layout_height="34dp"
                app:srcCompat="@drawable/logo_continue"
                android:id="@+id/imageView2"
                android:layout_marginTop="32dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:layout_marginEnd="36dp"/>

    </RelativeLayout>

    <LinearLayout
            android:orientation="horizontal"
            android:layout_width="0dp"
            android:layout_height="100dp"
            app:layout_constraintTop_toBottomOf="@+id/relativeLayout8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="5dp"
            app:layout_constraintHorizontal_bias="1.0"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:background="@drawable/frame_shape"
            android:id="@+id/linearLayout2">

        <RelativeLayout
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:background="#00F4EBEB">

            <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/button"
                    android:background="#00FFFFFF"/>

            <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/mine_func_image1"
                    app:srcCompat="@drawable/ic_launcher_foreground"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_below="@id/mine_func_image1"
                    android:textSize="18sp"
                    android:layout_centerHorizontal="true"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00FFFFFF">

            <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/button2"
                    android:background="#00FFFFFF"
                    />

            <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/mine_func_image2"
                    app:srcCompat="@drawable/tag_menu_circle"
                    />

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_below="@id/mine_func_image2"
                    android:textSize="18sp"
                    android:layout_centerHorizontal="true"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00FFFFFF">

            <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/button3"
                    android:backgroundTint="#00FFFFFF"
                    android:background="#00FFFFFF"/>

            <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    android:id="@+id/mine_func_image3"
                    app:srcCompat="@drawable/tag_menu_setting"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_below="@+id/mine_func_image3"
                    android:textSize="18sp"
                    android:layout_centerHorizontal="true"/>
        </RelativeLayout>

        <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:background="#00FFFFFF">

            <Button
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:id="@+id/button4"
                    android:background="#00FFFFFF"/>

            <ImageView
                    android:layout_width="50dp"
                    android:layout_height="50dp"
                    android:id="@+id/mine_func_image4"
                    android:layout_marginTop="10dp"
                    android:layout_alignParentTop="true"
                    android:layout_centerHorizontal="true"
                    app:srcCompat="@drawable/tag_menu_message"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="6dp"
                    android:layout_below="@id/mine_func_image4"
                    android:textSize="18sp"
                    android:layout_centerHorizontal="true"/>
        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@+id/linearLayout2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="16dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="24dp"
            >

        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:background="@drawable/frame_shape"
                android:layout_marginBottom="20dp"
                android:layout_weight="1"
                android:visibility="visible">

            <ImageView
                    android:layout_width="102dp"
                    android:layout_height="98dp"
                    android:id="@+id/imageView4"
                    android:layout_marginStart="18dp"
                    android:layout_centerVertical="true"
                    app:srcCompat="@drawable/unload"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView12"
                    android:layout_toEndOf="@+id/imageView4"
                    android:layout_marginStart="34dp"
                    android:layout_alignTop="@+id/imageView4"
                    android:layout_marginTop="13dp"
                    android:textSize="24sp"/>

            <TextView
                    android:text="TextView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:id="@+id/textView13"
                    android:layout_marginTop="8dp"
                    android:layout_below="@+id/textView12"
                    android:layout_marginStart="0dp"
                    android:layout_alignStart="@+id/textView12"
                    android:textSize="18sp"/>
        </RelativeLayout>

        <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="300dp"
                android:background="@drawable/frame_shape"
                android:layout_weight="1">

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <TextView
                        android:text="在线客服"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView14"
                        android:layout_marginStart="10dp"
                        android:textSize="20sp"
                        android:layout_toEndOf="@id/imageView5"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="31dp"
                        android:layout_height="30dp"
                        app:srcCompat="@android:drawable/ic_menu_mylocation"
                        android:id="@+id/imageView5"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        app:srcCompat="@drawable/logo_continue"
                        android:id="@+id/imageView6"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:layout_centerVertical="true"/>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <TextView
                        android:text="分享有礼"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView15"
                        android:layout_marginStart="10dp"
                        android:textSize="20sp"
                        android:layout_toEndOf="@+id/imageView7"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="31dp"
                        android:layout_height="30dp"
                        app:srcCompat="@android:drawable/ic_menu_set_as"
                        android:id="@+id/imageView7"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        app:srcCompat="@drawable/logo_continue"
                        android:id="@+id/imageView8"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:layout_centerVertical="true"/>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <TextView
                        android:text="反馈建议"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView16"
                        android:layout_marginStart="10dp"
                        android:textSize="20sp"
                        android:layout_toEndOf="@+id/imageView9"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="31dp"
                        android:layout_height="30dp"
                        app:srcCompat="@android:drawable/ic_dialog_email"
                        android:id="@+id/imageView9"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        app:srcCompat="@drawable/logo_continue"
                        android:id="@+id/imageView10"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:layout_centerVertical="true"/>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <TextView
                        android:text="软件更新"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView17"
                        android:layout_marginStart="10dp"
                        android:textSize="20sp"
                        android:layout_toEndOf="@+id/imageView11"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="31dp"
                        android:layout_height="30dp"
                        app:srcCompat="@android:drawable/ic_lock_idle_charging"
                        android:id="@+id/imageView11"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        app:srcCompat="@drawable/logo_continue"
                        android:id="@+id/imageView12"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:layout_centerVertical="true"/>
            </RelativeLayout>

            <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">

                <TextView
                        android:text="关于我们"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:id="@+id/textView18"
                        android:layout_marginStart="10dp"
                        android:textSize="20sp"
                        android:layout_toEndOf="@+id/imageView13"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="31dp"
                        android:layout_height="30dp"
                        app:srcCompat="@android:drawable/stat_notify_voicemail"
                        android:id="@+id/imageView13"
                        android:layout_alignParentStart="true"
                        android:layout_marginStart="10dp"
                        android:layout_centerVertical="true"/>

                <ImageView
                        android:layout_width="24dp"
                        android:layout_height="24dp"
                        app:srcCompat="@drawable/logo_continue"
                        android:id="@+id/imageView14"
                        android:layout_alignParentEnd="true"
                        android:layout_marginEnd="30dp"
                        android:layout_centerVertical="true"/>
            </RelativeLayout>
        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>